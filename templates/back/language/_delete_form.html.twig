<form method="post" action="{{ path('app_back_language_delete', {'id': language.id}) }}" onsubmit="return confirm('Etes-vous certain de vouloir supprimer ce Langage ?');">
    <input type="hidden" name="_token" value="{{ csrf_token('delete' ~ language.id) }}">
    <button class="btn my-2 col-4">Supprimer</button>
</form>
