<form method="post" action="{{ path('app_back_framework_delete', {'id': framework.id}) }}" onsubmit="return confirm('Etes-vous certain de vouloir supprimer ce Framework ?');">
    <input type="hidden" name="_token" value="{{ csrf_token('delete' ~ framework.id) }}">
    <button class="btn my-2 col-4">Supprimer</button>
</form>
